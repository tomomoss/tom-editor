{
  "devDependencies": {
    "css-loader": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.38.0",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.8.0"
  },
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:es-modules-type": "webpack --config ./webpack.config.module.js",
    "build:window-object-type": "webpack --config ./webpack.config.window.js",
    "watch": "npm-run-all --parallel watch:*",
    "watch:es-modules-type": "webpack --config ./webpack.config.module.js --watch",
    "watch:window-object-type": "webpack --config ./webpack.config.window.js --watch"
  }
}
